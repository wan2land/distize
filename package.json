{
  "name": "distize",
  "version": "2.0.0",
  "description": ".",
  "keywords": [
    "build",
    "node_modules",
    "distribute",
    "distribution",
    "awscdk",
    "cdk",
    "typescript"
  ],
  "author": "Changwan Jun <wan2land@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/wan2land/distize.git"
  },
  "bin": {
    "distize": "lib/bin/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/**/*.ts",
    "test:unit": "jest src --runInBand"
  },
  "dependencies": {
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "core-js": "^3.2.1",
    "glob": "^7.1.6",
    "ora": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/command-line-args": "5.0.0",
    "@types/command-line-usage": "5.0.1",
    "@types/glob": "7.1.2",
    "@types/jest": "25.2.3",
    "@types/node": "12.12.44",
    "eslint": "7.2.0",
    "eslint-config-stable": "^0.7.4",
    "jest": "26.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.1.0",
    "typescript": "3.9.5"
  }
}
